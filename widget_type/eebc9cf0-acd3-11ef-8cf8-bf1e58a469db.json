      "resources" : [ ],
      "templateHtml" : "<div class=\"content\" *ngIf=\"form\">\n    <span class=\"title\">Overview</span>\n    <div class=\"selectorBlock\" [formGroup]=\"form\">\n        <div class=\"block\">\n            <span class=\"sub-title\">LOCATION</span>\n            <mat-form-field class=\"tb-inline-field flex tb-suffix-absolute\" appearance=\"outline\">\n                <mat-select [compareWith]=\"compareFn\" formControlName=\"location\">\n                    <mat-option *ngFor=\"let location of locations\" [value]=\"location\">\n                        {{location.entityName}}\n\n                    </mat-option>\n                </mat-select>\n            </mat-form-field>\n        </div>\n        <div class=\"block\">\n            <span class=\"sub-title\">ZONE</span>\n            <mat-form-field class=\"tb-inline-field flex tb-suffix-absolute\" appearance=\"outline\">\n                <mat-select formControlName=\"zone\">\n                    <mat-option value=\"all\">All zones</mat-option>\n                    <mat-option *ngFor=\"let zone of zones\" [value]=\"zone\">\n                        {{zone.entityName}}\n                    </mat-option>\n                </mat-select>\n            </mat-form-field>\n        </div>\n    </div>\n</div>",
      "templateCss" : ".content {\n    display: flex;\n    align-items: center;\n    height: 100%;\n    width: 100%;\n    justify-content: space-between;\n}\n\n.selectorBlock {\n    display: flex;\n    align-items: center;\n}\n\n.block {\n    display: flex;\n    align-items: center;\n}\n\n.title {\n    font-size: 36px;\n    font-weight: 600;\n    line-height: 44px;\n    text-align: left;\n    color: #0C111D;\n}\n\n.sub-title {\n    font-size: 14px;\n    font-weight: 400;\n    line-height: 20px;\n    text-align: left;\n    color: #667085;\n    margin-right: 10px;\n    margin-left: 10px;\n}\n\n.mat-mdc-form-field-subscript-wrapper, .tb-default .mat-mdc-form-field-bottom-align:before {\n    display: none;\n}\n\n.mat-mdc-text-field-wrapper {\n    background-color: white;\n    border-radius: 3px;\n}\n\n.mat-mdc-form-field {\n    min-width: 200px;\n}",
      "controllerScript" : "let entityService, broadcastService, controller, params, alias;\n\nself.onInit = function() {\n    entityService = self.ctx.$scope.$injector.get(self.ctx.servicesMap.get('entityService'));\n    broadcastService = self.ctx.$scope.$injector.get(self.ctx.servicesMap.get('broadcastService'));\n    \n    controller = self.ctx.stateController;\n    params = controller.getStateParams();\n    alias = Object.values(self.ctx.aliasController.entityAliases).find(el => el.alias === 'Selected entity assets');\n    \n    self.ctx.$scope.locations = [];\n    self.ctx.$scope.zones = [];\n    self.ctx.$scope.currentLocation;\n    \n    self.ctx.$scope.form = self.ctx.$scope.fb.group({\n        location: params.selectedEntity ? params.selectedEntity : null,\n        zone: [\"all\"]\n    });\n    \n    self.ctx.$scope.compareFn = function(c1, c2) {\n        return c1 && c2 ? c1.entityId.id === c2.entityId.id : c1 === c2;\n    };\n    \n    const pageLink = self.ctx.pageLink(123123);\n    \n    self.ctx.$scope.form.controls.location.valueChanges.subscribe(value=>{\n        const query = {\n            entityFilter: {\n                \"type\": \"relationsQuery\",\n                \"resolveMultiple\": true,\n                \"rootStateEntity\": false,\n                \"stateEntityParamName\": null,\n                \"defaultStateEntity\": null,\n                \"rootEntity\": value.entityId,\n                \"direction\": \"TO\",\n                \"maxLevel\": 1,\n                \"fetchLastLevelOnly\": false,\n                \"filters\": [\n                    {\n                      \"relationType\": \"ZoneToLocation\",\n                      \"entityTypes\": [\n                        \"ASSET\"\n                      ]\n                    }\n                ]\n            },\n            entityFields: [{\n              \"type\": \"ENTITY_FIELD\",\n              \"key\": \"name\"\n            }],\n            pageLink\n        };\n        \n        entityService.findEntityDataByQuery(query).subscribe(zones => {\n            self.ctx.$scope.zones = [];\n            if(zones && zones.data){\n                zones.data.forEach(zone=>{\n                    self.ctx.$scope.zones.push({\n                        entityId: zone.entityId,\n                        entityName: zone.latest.ENTITY_FIELD.name.value\n                    });\n                });\n            }\n        });\n        \n        changeAlias(alias, \"FROM\", \"LocationToAsset\");\n        changeSelectedEntity(value);\n        self.ctx.$scope.currentLocation = value;\n        \n        self.ctx.$scope.form.patchValue({\n            zone: \"all\"\n        });\n        \n        self.ctx.detectChanges();\n    });\n    \n    self.ctx.$scope.form.controls.zone.valueChanges.subscribe(value=>{\n        if(value != \"all\"){\n            changeAlias(alias, \"TO\", \"AssetToZone\");\n            changeSelectedEntity(value);\n        } else {\n            changeAlias(alias, \"FROM\", \"LocationToAsset\");\n            changeSelectedEntity(self.ctx.$scope.currentLocation);\n        }\n    });\n};\n\nfunction changeSelectedEntity(entity){\n    let newParams = {\n        stateId: 'monitoring',\n        prevState: 'monitoring',\n        subState: '',\n        mapCentered: false,\n        statesArray: [\n            {\n                stateId: 'monitoring', \n                name: \"Monitoring\"\n            }\n        ],\n        selectedEntity: {\n            entityName: entity.entityName,\n            entityId: entity.entityId\n        }\n    };\n    \n    controller.updateState('default', newParams);\n}\n\nfunction changeAlias(aliasToChange, direction, relationType){\n    if(aliasToChange){\n        aliasToChange.filter.direction = direction;\n        aliasToChange.filter.filters[0].relationType = relationType;\n        self.ctx.updateAliases([aliasToChange.id]);\n    }\n}\n\nself.onDataUpdated = function() {\n    self.ctx.$scope.locations = [];\n\n    self.ctx.data.forEach(x=>{\n        self.ctx.$scope.locations.push({\n            entityName: x.datasource.name,\n            entityId: x.datasource.entity.id\n        });\n    });\n    \n    if(!self.ctx.dashboard.isEdit && self.ctx.$scope.locations.length){\n        self.ctx.$scope.form.patchValue({\n            location: self.ctx.$scope.locations[self.ctx.$scope.locations.length-1],\n        });\n    }\n};\n\nself.onResize = function() {\n};\n\nself.onDestroy = function() {\n    // changeAlias(alias, \"FROM\", \"LocationToAsset\");\n    // changeSelectedEntity(self.ctx.$scope.currentLocation);\n};\n",
