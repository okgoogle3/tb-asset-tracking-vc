      "controllerScript" : "self.onInit = function() {\n    utils = self.ctx.$scope.$injector.get(self.ctx.servicesMap.get('utils'));\n    settings = utils.deepClone(self.ctx.settings) || {};\n    \n    self.ctx.$scope.type = settings.type;\n    self.ctx.$scope.datasource = [];\n    self.ctx.$scope.displayedColumns = [];\n    self.ctx.$scope.noData = false;\n    \n    if(settings.type == \"Device thresholds\"){\n        self.ctx.$scope.datasource = [];\n        \n        self.ctx.$scope.displayedColumnsTable = [\n            {key: \"type\", label: \"Type\", isAction: false}, \n            {key: \"upperThreshold\", label: \"Upper Threshold\", isAction: false}, \n            {key: \"lowerThreshold\", label: \"Lower Threshold\", isAction: false}, \n            {key: \"equalThreshold\", label: \"Equal Threshold\", isAction: false}, \n            {key: \"durationThreshold\", label: \"Duration Threshold\", isAction: false}, \n            {key: \"status\", label: \"Status\", isAction: false}, \n            {key: \"edit\", label: \"Edit\", isAction: true, actionKey: \"edit\"}\n        ];\n        \n        self.ctx.$scope.displayedColumns = [\"type\", \"upperThreshold\", \"lowerThreshold\", \"equalThreshold\", \"durationThreshold\", \"status\", \"edit\"];\n    } else {\n        self.ctx.$scope.datasource = [];\n        \n        self.ctx.$scope.displayedColumnsTable = [\n            {key: \"eventType\", label: \"Event Type\", isAction: false}, \n            {key: \"duration\", label: \"Duration\", isAction: false}, \n            {key: \"edit\", label: \"Edit\", isAction: true, actionKey: \"edit\"},\n            {key: \"delete_outline\", label: \"Delete\", isAction: true, actionKey: \"delete_outline\"}\n        ];\n        \n        self.ctx.$scope.displayedColumns = [\"eventType\", \"duration\", \"edit\", \"delete\"];\n    }\n    \n    self.ctx.$scope.edit = function(element){\n        let actionEventDescriptors = self.ctx.$scope.ctx.actionsApi.getActionDescriptors('elementClick');\n        let actionEvent;\n            \n        if (actionEventDescriptors.length) {\t\n            actionEventDescriptors.forEach(action=>{\n                if(action.name == 'edit'){\n                    actionEvent = action;\n                }\n            });\n                \n            setTimeout(() => {\n                self.ctx.$scope.ctx.actionsApi.handleWidgetAction(null, actionEvent, null, null, element);\t\n            }, 0);\t\n        }\n    };\n    \n    self.ctx.$scope.delete = function(element){\n        let actionEventDescriptors = self.ctx.$scope.ctx.actionsApi.getActionDescriptors('elementClick');\n        let actionEvent;\n            \n        if (actionEventDescriptors.length) {\t\n            actionEventDescriptors.forEach(action=>{\n                if(action.name == 'delete'){\n                    actionEvent = action;\n                }\n            });\n                \n            setTimeout(() => {\n                self.ctx.$scope.ctx.actionsApi.handleWidgetAction(null, actionEvent, null, null, element);\t\n            }, 0);\t\n        }\n    };\n};\n\nself.onDataUpdated = function() {\n    self.ctx.$scope.datasource = [];\n    \n    if(self.ctx.data[0].data[0]){\n        if(self.ctx.data[0].data[0][1] && JSON.parse(self.ctx.data[0].data[0][1])){\n            self.ctx.$scope.alarmConfig = JSON.parse(self.ctx.data[0].data[0][1]);\n            if(self.ctx.$scope.type == \"Device thresholds\"){\n                if(self.ctx.$scope.alarmConfig.deviceThresholds){\n                    self.ctx.$scope.datasource = self.ctx.$scope.alarmConfig.deviceThresholds;\n                } else {\n                    self.ctx.$scope.alarmConfig.deviceThresholds = [];\n                    \n                    self.ctx.$scope.alarmConfig.deviceThresholds.push({\n                        type: \"Temperature\",\n                        status: false,\n                        upperThreshold: null,\n                        lowerThreshold: null,\n                        equalThreshold: null,\n                    });\n                    self.ctx.$scope.alarmConfig.deviceThresholds.push({\n                        type: \"Battery level\",\n                        status: false,\n                        upperThreshold: null,\n                        lowerThreshold: null,\n                        equalThreshold: null,\n                    });\n                    self.ctx.$scope.alarmConfig.deviceThresholds.push({\n                        type: \"Movement\",\n                        status: false,\n                        durationThreshold: null\n                    });\n                    self.ctx.$scope.alarmConfig.deviceThresholds.push({\n                        type: \"Idle\",\n                        status: false,\n                        durationThreshold: null\n                    });\n                    self.ctx.$scope.datasource = self.ctx.$scope.alarmConfig.deviceThresholds;\n                    self.ctx.$scope.displayedColumns = [\"type\", \"upperThreshold\", \"lowerThreshold\", \"equalThreshold\", \"durationThreshold\", \"status\", \"edit\"];\n\n                    self.ctx.$scope.displayedColumnsTable = [\n                        {key: \"type\", label: \"type\", isAction: false}, \n                        {key: \"upperThreshold\", label: \"Upper Threshold\", isAction: false}, \n                        {key: \"lowerThreshold\", label: \"Lower Threshold\", isAction: false}, \n                        {key: \"equalThreshold\", label: \"Equal Threshold\", isAction: false}, \n                        {key: \"durationThreshold\", label: \"DurationThreshold\", isAction: false}, \n                        {key: \"status\", label: \"Status\", isAction: false}, \n                        {key: \"edit\", label: \"Edit\", isAction: true, actionKey: \"edit\"},\n                    ];\n                }\n            } else {\n                if(self.ctx.$scope.alarmConfig.locationThresholds && self.ctx.$scope.alarmConfig.locationThresholds.length){\n                    self.ctx.$scope.datasource = self.ctx.$scope.alarmConfig.locationThresholds;\n                    self.ctx.$scope.displayedColumnsTable = [\n                        {key: \"eventType\", label: \"Event Type\", isAction: false}, \n                        {key: \"duration\", label: \"Duration\", isAction: false}, \n                        {key: \"edit\", label: \"Edit\", isAction: true, actionKey: \"edit\"},\n                        {key: \"delete_outline\", label: \"Delete\", isAction: true, actionKey: \"delete_outline\"}\n                    ];\n                    self.ctx.$scope.displayedColumns = [\"eventType\", \"duration\", \"edit\", \"delete_outline\"];\n                } else {\n                    self.ctx.$scope.noData = true;\n                }\n            }\n        }\n    }\n};\n\nself.actionSources = function() {\n    return {\n        'elementClick': {\n            name: 'widget-action.element-click',\n            multiple: true\n        }\n    };\n};\n\nself.onResize = function() {\n  \n};\n\nself.onDestroy = function() {\n};\n",
