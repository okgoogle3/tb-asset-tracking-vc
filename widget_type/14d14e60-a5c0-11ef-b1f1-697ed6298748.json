      "templateCss" : ".card-container {\n    display: flex;\n    width: 100%;\n    height: 100%;\n}\n\n.icon-wrapper {\n    flex: 1;\n    align-self: center;\n    padding: 20px;\n}\n\n.description-wrapper {\n    flex: 3;\n}\n\n.description-title {\n    color: #0C111D;\n    font-size: 24px;\n    font-weight: 600;\n    line-height: 32px;\n    margin-bottom: 8px;\n}\n\n.description-text {\n    color: #344054;\n    line-height: 24px;\n    font-size: 14px;\n}\n\n.description-wrapper {\n    height: 100%;\n    overflow: auto;\n}",
      "controllerScript" : "function getObjectByLabel(arr, label) {\n    return arr.find((element) => element.dataKey.label === label);\n}\n\nself.onInit = function() {\n    let utils = self.ctx.$scope.$injector.get(self.ctx.servicesMap.get('utils'));\n    settings = utils.deepClone(self.ctx.settings) || {};\n    \n    self.ctx.$scope.settings = settings;\n    \n    const imageObj = getObjectByLabel(self.ctx.data, \"image\");\n    \n    if (imageObj && imageObj.data && imageObj.data.length) {\n        self.ctx.$scope.imageSrc = imageObj.data[0][1];\n    }\n    \n    const descriptionObj = getObjectByLabel(self.ctx.data, \"description\");\n    \n    if (descriptionObj && descriptionObj.data && descriptionObj.data.length) {\n        self.ctx.$scope.description = descriptionObj.data[0][1];\n    }\n}\n\nself.onDataUpdated = function() {\n}\n\nself.onDestroy = function() {\n}\n",
