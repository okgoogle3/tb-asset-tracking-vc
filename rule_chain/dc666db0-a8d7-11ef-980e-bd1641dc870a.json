        "jsScript" : "return {msg: msg, metadata: metadata, msgType: msgType};",
        "tbelScript" : "var result = {};\n\n\nvar thresholds = msg.deviceThresholds;\nif(thresholds!=null){\n    foreach(threshold : thresholds){\n    if(threshold.type == \"temperature\" || threshold.type ==  \"batteryLevel\"){\n    result.put(\"upperThreshold\" + threshold.type + \"Enabled\",threshold.status && msg.isActive);\n    result.put(\"lowerThreshold\" + threshold.type + \"Enabled\",threshold.status && msg.isActive);\n    result.put(\"equalThreshold\" + threshold.type + \"Enabled\",threshold.status && msg.isActive);\n    \n    result.put(\"upperThreshold\" + threshold.type,threshold.upperThreshold);\n    result.put(\"lowerThreshold\" + threshold.type,threshold.lowerThreshold);\n    result.put(\"equalThreshold\" + threshold.type,threshold.equalThreshold);\n    }else if (threshold.type == \"movement\" || threshold.type == \"idle\"){\n        result.put(threshold.type + \"Enabled\",threshold.status && msg.isActive);\n        result.put(threshold.type + \"TimeSec\",threshold.timeSec);\n\n    }\n}\n}\n\n\nvar zoneThresolds = {};\nvar geofenceThresolds = {};\nvar areaAlarmRules = {};\n\nif( msg.alarmConfig.locationThresholds!=null){\n     foreach(locationThreshold : msg.alarmConfig.locationThresholds){\n         \nif (locationThreshold.eventType.contains(\"Geofence\")) {\n    processThresholds(locationThreshold.geofences, geofenceThresolds, locationThreshold.eventType, locationThreshold.duration);\n}\n\nif (locationThreshold.eventType.contains(\"Zone\")) {\n    processThresholds(locationThreshold.zones, zoneThresolds, locationThreshold.eventType, locationThreshold.duration);\n}\n     \n }\n}\n\n \n result.put(\"geofenceThresolds\",geofenceThresolds);\n result.put(\"zoneThresolds\",zoneThresolds);\n result.put(\"areaAlarmRules\",areaAlarmRules);\n\n\n\n\nreturn {msg: result, metadata: metadata, msgType: \"POST_ATTRIBUTES_REQUEST\"};\n\n\nfunction processThresholds(thresholdList, thresholdMap, eventType, durationKey) {\n    if (thresholdList != null) {\n        foreach (item : thresholdList) {\n            var itemId = item.id.id;\n            boolean insideEvent = eventType.contains(\"In\");\n            var type = insideEvent ? \"minInsideDuration\" : \"minOutsideDuration\";\n            areaAlarmRules.put(itemId,{\"name\":item.name,\"status\" : (insideEvent ? \"inside\" : \"outside\")});\n            var jsn = {};\n            if (thresholdMap[itemId] == null) {\n                jsn[type] = durationKey;\n                thresholdMap.put(itemId, jsn);\n            } else {\n                jsn = thresholdMap[itemId];\n                jsn[type] = durationKey;\n            }\n        }\n    }\n}"
