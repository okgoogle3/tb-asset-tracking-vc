        "scriptLang" : "TBEL",
        "jsScript" : "return {msg: msg, metadata: metadata, msgType: msgType};",
        "tbelScript" : "metadata.configUpdated = false;\nvar alarmConfig = JSON.parse(metadata.ss_alarmConfig);\n\nif (alarmConfig.locationThresholds != null) {\n    foreach(locationThreshold: alarmConfig.locationThresholds) {\n        if (locationThreshold.eventType.contains(\"Zone\")) {\n            var filteredZones = [];\n            for(var i = 0; i < locationThreshold.zones.length; i++) {\n                if (locationThreshold.zones[i].id.id != msg.id) {\n                 filteredZones.push(locationThreshold.zones[i]);\n                }else{\n                    metadata.configUpdated = true;\n                }\n            }\n            locationThreshold.zones = filteredZones;\n        }\n\n    }\n}\nreturn {\n    msg: {alarmConfig:alarmConfig},\n    metadata: {},\n    msgType: msgType\n};"
      },
