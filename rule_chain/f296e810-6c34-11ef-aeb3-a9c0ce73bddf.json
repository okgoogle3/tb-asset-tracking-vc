      "configuration" : {
        "scriptLang" : "TBEL",
        "jsScript" : "return {msg: msg, metadata: metadata, msgType: msgType};",
        "tbelScript" : "var configUpdated = false;\nvar alarmConfig = JSON.parse(metadata.ss_alarmConfig);\nvar test = [];\nif (alarmConfig.locationThresholds != null) {\n    var locationThresholdsToRemove = [];\n    foreach(locationThreshold: alarmConfig.locationThresholds) {\n        if (locationThreshold.eventType.contains(\"Zone\")) {\n            var zonesToRemove = [];\n            foreach(zone : locationThreshold.zones) {\n                if (zone.id.id == msg.id.id) {\n                 zonesToRemove.push(zone);\n                     configUpdated = true;\n                }\n            }\n                 foreach(remove : zonesToRemove){\n                locationThreshold.zones.remove(remove);\n          }\n\n        } else if (locationThreshold.eventType.contains(\"Geofence\")) {\n              if(locationThreshold.zones.id.id == msg.id.id){\n                  locationThresholdsToRemove.push(locationThreshold);\n                  configUpdated = true;\n              } else {\n                  var geofencesToRemove = [];\n                  foreach(geofence: locationThreshold.geofences){\n                      if(geofence.id.id == msg.id.id){\n                         geofencesToRemove.push(geofence);\n                         configUpdated = true; \n                      }\n                  }\n                  foreach(remove: geofencesToRemove){\n                      locationThreshold.geofences.remove(remove);\n                  }\n              }\n          }\n          \n      \n          \n    }\n \n   \n    foreach(remove : locationThresholdsToRemove){\n        alarmConfig.locationThresholds.remove(remove);\n    }\n}\nreturn {\n    msg: {alarmConfig:alarmConfig},\n    metadata: {\n        configUpdated : configUpdated\n    },\n    msgType: \"POST_ATTRIBUTES_REQUEST\"\n};"
      },
